  "training": {
    "learning_rate": 0.0003,
    "min_learning_rate": 0.00001,
    "weight_decay": 0.01,
    "warmup_steps": 10,
    "max_steps": 50,
    "batch_size": 4,
    "gradient_accumulation_steps": 2,
    "checkpoint_dir": "./checkpoints/ray-tiny",
    "save_every_n_steps": 25,
    "log_every_n_steps": 10,
    "eval_every_n_steps": 25
  }
